{
  "name": "s3-sink-gr-table-data-connector",
  "connector.class": "io.confluent.connect.s3.S3SinkConnector",
  "aws.access.key.id": "minioadmin",
  "aws.secret.access.key": "minioadmin",
  "store.url": "http://minio:9000",
  "s3.bucket.name": "my-bucket",
  "topics.dir": "user_events",
  "s3.region": "us-east-1",
  "storage.class": "io.confluent.connect.s3.storage.S3Storage",
  "topics": "user_events",
  "value.converter": "io.confluent.connect.avro.AvroConverter",
  "value.converter.schema.registry.url": "http://schema-registry:8081",
  "key.converter": "org.apache.kafka.connect.storage.StringConverter",
  "format.class": "io.confluent.connect.s3.format.parquet.ParquetFormat",
  "parquet.codec": "gzip",
  "flush.size": "1000",
  "rotate.schedule.interval.ms": "300000",
  "partitioner.class": "io.confluent.connect.storage.partitioner.TimeBasedPartitioner",
  "path.format": "'year'=YYYY/'month'=MM/'day'=dd",
  "partition.duration.ms": "3600000",
  "timestamp.extractor": "Wallclock",
  "errors.tolerance": "all",
  "errors.deadletterqueue.topic.name": "user_events-dlq",
  "errors.deadletterqueue.topic.replication.factor": "1",
  "errors.deadletterqueue.context.headers.enable": "true",
  "transforms": "InsertEnvInfo,InsertTimeStamp,tsFormat",
  "transforms.InsertEnvInfo.type": "org.apache.kafka.connect.transforms.InsertField$Value",
  "transforms.InsertEnvInfo.static.field": "env",
  "transforms.InsertEnvInfo.static.value": "test",
  "transforms.InsertTimeStamp.type": "org.apache.kafka.connect.transforms.InsertField$Value",
  "transforms.InsertTimeStamp.timestamp.field": "etl_timestamp",
  "transforms.tsFormat.type": "org.apache.kafka.connect.transforms.TimestampConverter$Value",
  "transforms.tsFormat.field": "etl_timestamp",
  "transforms.tsFormat.target.type": "string",
  "transforms.tsFormat.format": "yyyy-MM-dd'T'HH:mm:ss.SSS'Z'",
  "timezone": "UTC",
  "tasks.max": "20",
  "locale": "en-US"
}
